{
    "env": {
      "baseURL": "http://the-internet.herokuapp.com/"
    },
    "pageLoadTimeout": 90000,
    "defaultCommandTimeout": 10000,
    "reporter": "mochawesome",
    "reporterOptions": {
      "reportDir": "cypress/reports/mocha",
      "reportFilename": "report",
      "overwrite": true,
      "html": true,
      "json": true,
      "charts": true
    },
    "retries": {
      "runMode": 1,
      "openMode": 1
    },
    "video": true
  }